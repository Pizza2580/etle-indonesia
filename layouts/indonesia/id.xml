<layouts>
        <layout name="root">
		<row>
			<button type="picture" icon="logo_camera"/>
			<button type="textnote" icon="logo_textnote"/>
			<button type="voicerec" icon="logo_voice"/>
		</row>
		<row>
			<button type="page" label="Landscape" targetlayout="landscape"/>
			<button type="page" label="Portrait" targetlayout="portrait"/>
		</row>
        </layout>
	<layout name="portrait">
		<row>
			<button type="tag" label="Tengah Jalan - Depan" icon="etle_icons/etlemiddlefront.png"/>
			<button type="tag" label="Tengah Jalan - Belakang" icon="etle_icons/etlemiddleback.png"/>
			<button type="tag" label="Tengah Jalan - Keduanya" icon="etle_icons/etlemiddleboth.png"/>
		</row>
		<row>
			<button type="tag" label="Tengah Jalan - Kubah" icon="etle_icons/etlemiddledome.png"/>
			<button type="tag" label="Jalan - Depan" icon="etle_icons/etleroadfront.png"/>
			<button type="tag" label="Jalan - Belakang" icon="etle_icons/etleroadback.png"/>
		</row>
		<row>
			<button type="tag" label="Jalan - Keduanya" icon="etle_icons/etleroadboth.png"/>
			<button type="tag" label="Jalan - Kubah" icon="etle_icons/etleroaddome.png"/>
			<button type="tag" label="Jalan Lain - Depan" icon="etle_icons/etleotherroadfront.png"/>
		</row>
		<row>
			<button type="tag" label="Jalan Lain - Belakang" icon="etle_icons/etleotherroadback.png"/>
			<button type="tag" label="Jalan Lain - Keduanya" icon="etle_icons/etleotherroadboth.png"/>
			<button type="tag" label="Jalan Lain - Kubah" icon="etle_icons/etleotherroaddome.png"/>
		</row>
		<row>
			<button type="tag" label="Rambu Peringatan ETLE" icon="etle_icons/cctvspeedwarning.png"/>
		</row>
        </layout>
	<layout name="landscape">
		<row>
			<button type="tag" label="Tengah Jalan - Depan" icon="etle_icons/etlemiddlefront.png"/>
			<button type="tag" label="Tengah Jalan - Belakang" icon="etle_icons/etlemiddleback.png"/>
			<button type="tag" label="Tengah Jalan - Keduanya" icon="etle_icons/etlemiddleboth.png"/>
			<button type="tag" label="Tengah Jalan - Kubah" icon="etle_icons/etlemiddledome.png"/>
			<button type="tag" label="Jalan - Depan" icon="etle_icons/etleroadfront.png"/>
		</row>
		<row>
			<button type="tag" label="Jalan - Belakang" icon="etle_icons/etleroadback.png"/>
			<button type="tag" label="Jalan - Keduanya" icon="etle_icons/etleroadboth.png"/>
			<button type="tag" label="Jalan - Kubah" icon="etle_icons/etleroaddome.png"/>
			<button type="tag" label="Jalan Lain - Depan" icon="etle_icons/etleotherroadfront.png"/>
			<button type="tag" label="Jalan Lain - Belakang" icon="etle_icons/etleotherroadback.png"/>
		</row>
		<row>
			<button type="tag" label="Jalan Lain - Keduanya" icon="etle_icons/etleotherroadboth.png"/>
			<button type="tag" label="Jalan Lain - Kubah" icon="etle_icons/etleotherroaddome.png"/>
			<button type="tag" label="Rambu Peringatan ETLE" icon="etle_icons/cctvspeedwarning.png"/>
		</row>
		
	</layout>
</layouts>