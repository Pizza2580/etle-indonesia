<layouts>
        <layout name="root">
		<row>
		    <button type="picture" icon="logo_camera"/>
		    <button type="textnote" icon="logo_textnote"/>
		    <button type="voicerec" icon="logo_voice"/>
		</row>
		<row>
		    <button type="page" label="Kamera Kecepatan" targetlayout="kecepatan"/>
		    <button type="page" label="Kamera Lampu Lalu Lintas" targetlayout="lalu_lintas"/>
	            <button type="page" label="Rambu-Rambu" targetlayout="rambu"/>
		</row>
        </layout>
	<layout name="kecepatan">
		<row>
		    <button type="tag" label="Tengah Jalan - Depan" icon="etle_icons/etlemiddlefront.png"/>
		    <button type="tag" label="Tengah Jalan - Belakang" icon="etle_icons/etlemiddleback.png"/>
		    <button type="tag" label="Tengah Jalan - Keduanya" icon="etle_icons/etlemiddleboth.png"/>
		    <button type="tag" label="Tengah Jalan - Kubah" icon="etle_icons/etlemiddledome.png"/>
		</row>
		<row>
		    <button type="tag" label="Jalan - Depan" icon="etle_icons/etleroadfront.png"/>
		    <button type="tag" label="Jalan - Belakang" icon="etle_icons/etleroadback.png"/>
		    <button type="tag" label="Jalan - Keduanya" icon="etle_icons/etleroadboth.png"/>
		    <button type="tag" label="Jalan - Kubah" icon="etle_icons/etleroaddome.png"/>
		</row>
		<row>
		    <button type="tag" label="Jl. Lain - Depan" icon="etle_icons/etleotherroadfront.png"/>
		    <button type="tag" label="Jl. Lain - Belakang" icon="etle_icons/etleotherroadback.png"/>
		    <button type="tag" label="Jl. Lain - Keduanya" icon="etle_icons/etleotherroadboth.png"/>
		    <button type="tag" label="Jl. Lain - Kubah" icon="etle_icons/etleotherroaddome.png"/>
		</row>
        </layout>
	<layout name="lalu_lintas">
		<row>
		    <button type="tag" label="Jalan - Depan" icon="etle_icons/etleredlightfront.png"/>
		    <button type="tag" label="Jalan - Belakang" icon="etle_icons/etleredlightback.png"/>
		    <button type="tag" label="Jalan - Keduanya" icon="etle_icons/etleredlightboth.png"/>
		    <button type="tag" label="Jalan - Kubah" icon="etle_icons/etleredlightdome.png"/>
		</row>
        </layout>
	<layout name="rambu">
		<row>
                    <button type="tag" label="Rambu Peringatan ETLE" icon="etle_icons/cctvspeedwarning.png"/>
		    <button type="tag" label="Rambu Radar Kecepatan" icon="etle_icons/radarspeedsign.png"/>
		</row>
	</layout>
	
</layouts>
